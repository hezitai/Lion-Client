       window.addEventListener('setItem', (e) => {\n            // console.log(e);\n            if (e.key == 'messageSetData') {\n\n            } else if (e.key == 'sendSetData') {\n\n            } else if (e.key == 'sendNavList') {\n                this.sendNavList = JSON.parse(e.newValue);\n            } else if (e.key == 'messageNavList') {\n                this.messageNavList = JSON.parse(e.newValue);\n            }\n            // this.sendNavList = JSON.parse(e.newValue);\n            // this.messageNavList = JSON.parse(localStorage.getItem(e.key));\n        });\n        // this.messageOptionForm = JSON.parse(localStorage.getItem('messageSetData'));\n\n        // if (!this.sendNavList) {\n        //     this.sendNavList = []\n        //     this.sendNavList.push({\n        //         id: 99999,\n        //         name: '+'\n        //     });\n        // }\n        // if (!this.messageNavList) {\n        //     this.messageNavList = [];\n        //     this.messageNavList.push({\n        //         id: 99999,\n        //         name: '+'\n        //     });\n        // }\n\n        // this.presentMsgNav = '0';\n        // this.presentNav = '0';\n        /**\n         * Vuex \n        */\n        this.$store.commit('edit', { age: 15, sex: 'male' })\n        // console.log(this.$store.getters.fullInfo);\n        this.$set(this.$store.state.oneState, 'own', 'hezitai');\n        // this.$set(this.$store.state.oneState,'sex')\n        // this.$delete(this.$store.state.oneState, 'name');\n        // console.log(this.$store.state);\n        this.$store.dispatch('aEdit', { age: 16 })\n\n        /**\n         * 测试场景\n         * 修改NAVname  添加备注\n         * */\n    },\n    components: {\n        vueJsonEditor\n    },\n    methods: Methods\n}\n</script>\n<style>\n.el-divider--horizontal {\n    margin: 12px 0;\n}\n#index {\n    width: 100%;\n    height: 100%;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n}\n.card {\n    width: 80%;\n    hei