orage.getItem(e.key));\n        });\n        // this.messageOptionForm = JSON.parse(localStorage.getItem('messageSetData'));\n\n        // if (!this.sendNavList) {\n        //     this.sendNavList = []\n        //     this.sendNavList.push({\n        //         id: 99999,\n        //         name: '+'\n        //     });\n        // }\n        // if (!this.messageNavList) {\n        //     this.messageNavList = [];\n        //     this.messageNavList.push({\n        //         id: 99999,\n        //         name: '+'\n        //     });\n        // }\n\n        // this.presentMsgNav = '0';\n        // this.presentNav = '0';\n        /**\n         * Vuex \n        */\n        this.$store.commit('edit', { age: 15, sex: 'male' })\n        // console.log(this.$store.getters.fullInfo);\n        this.$set(this.$store.state.oneState, 'own', 'hezitai');\n        // this.$set(this.$store.state.oneState,'sex')\n        // this.$delete(this.$store.state.oneState, 'name');\n        // console.log(this.$store.state);\n        this.$store.dispatch('aEdit', { age: 16 })\n\n        /**\n         * 测试场景\n         * 修改NAVname  添加备注\n         * */\n    },\n    components: {\n        vueJsonEditor\n    },\n    methods: Methods\n}\n</script>\n<style>\n.el-divider--horizontal {\n    margin: 12px 0;\n}\n#index {\n    width: 100%;\n    height: 100%;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n}\n.card {\n    width: 80%;\n    height: 80%;\n}\n.plan-item {\n    padding: 20px 0;\n}\n.item-content {\n    width: 100%;\n    height: 100%;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n}\n.plan-form {\n    padding-right: 20px;\n}\n.send-item,\n.message-item {\n    height: 100%;\n}\n.el-menu-vertical-demo {\n    height: 100%;\n}\n.send-item-left {\n    height: 100%;\n    width: 150px;\n    overflow: auto;\n    border-right: 1px solid #eee;\n}\n.send-item-left::-webkit-scrollbar {\n    display: none; /* Chrome Safari */\n}\n.send-item-ri